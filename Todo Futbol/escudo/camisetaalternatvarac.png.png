    <script>
        var tmrReady = setInterval(isPageFullyLoaded, 100);

        function isPageFullyLoaded() {
            if (document.readyState == "loaded" || document.readyState == "complete") {
                subclassForms();
                clearInterval(tmrReady);
            }
        }

        function submitDisabled(_form, currSubmit) {
            return function () {
                var mustSubmit = true;
                if (currSubmit != null)
                    mustSubmit = currSubmit();

                var els = _form.elements;
                for (var i = 0; i < els.length; i++) {
                    let element = els[i];
                    if (element.type == "submit")
                        if (mustSubmit) {
                            element.disabled = true;
                            setTimeout(function () {
                                enableSubmit(element)
                            }, 7000);
                        }
                }
                return mustSubmit;
            }
        }

        function subclassForms() {
            for (var f = 0; f < document.forms.length; f++) {
                var frm = document.forms[f];
                frm.onsubmit = submitDisabled(frm, frm.onsubmit);
            }
        }

        var enableSubmit = function (ele) {
            $(ele).attr("disabled", false);
        }

        /* Menu fix */
        window.onscroll = function () {
            myFunction()
        };

        var navbar = document.querySelector("header .main");
        var sticky = navbar.offsetTop;
        var section = document.querySelector("section");
        //var navheight = $('.menues').outerHeight();

        // agrega sticky
        function myFunction() {
            if (window.pageYOffset > sticky) {
                navbar.classList.add("sticky")
                section.style.paddingTop; 
            }
            else {
                navbar.classList.remove("sticky");
                section.style.paddingTop = "0"
            }
        }

        $(".Hamburguer-desktop").click(function () {
            $(".navbar-header.main").toggleClass("dark")
        });

        $(".MENU_MAIN .nav > li > a").hover(function () {
            var id = $(this).parent("li").attr("id").replace('cat-item-', '');
            $(".MENU_MAIN .nav > li > a").removeClass("active");
            $(this).addClass("active");
            $(".subgroup").hide();
            $(".subgroup#cat-subitem-" + id).show()
        })

        /* abre-cierra buscador */
        $('.lupa2.nop').on('click', function () {
            $(".SEARCH_BOX").addClass("show2");
        });

        $('.lupa.sip').on('click', function () {
            $(".SEARCH_BOX").removeClass("show2");
            $(".lupa").removeClass("sip");
            $(".lupa").addClass("nop");
        });

        $('.lupaclose').on('click', function () {
            $(".SEARCH_BOX").removeClass("show2");
        });

        $(window).resize(function () { //begin resize event
            var winWidth = $(window).width();
            var maxWidth = 992;

            if (winWidth < maxWidth) {
                $(".SEARCH_BOX").removeClass("show2");
                $(".lupa").removeClass("sip");
                $(".lupa").addClass("nop");
            }
        });

        // Oculta banner header up
        if (sessionStorage.getItem('banner-up') == 'true') {
                $( ".upper > div" ).removeClass( "show" ).addClass( "hide" );
        } 
        $(".upper .closeupper").click(function() {
            if (sessionStorage.getItem('banner-up') !== 'true') {
                sessionStorage.setItem('banner-up','true');
                $( ".upper > div" ).removeClass( "show" ).addClass( "hide" );
            } 
        });
    </script>

